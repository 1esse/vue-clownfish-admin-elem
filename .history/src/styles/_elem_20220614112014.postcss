.el-menu{
    background-color: inherit;
    border-right: none;
}

.el-breadcrumb__item{
    &:last-child{
        & .el-breadcrumb__inner{
            font-weight: 400;
    color: var(--el-text-color-regular);
    cursor: text;
        }
    }
}  

.el-breadcrumb__inner a {
    color: #00000073;
    font-weight: normal;
    text-decoration: none;
    -webkit-transition: var(--el-transition-color);
    transition: var(--el-transition-color);
}